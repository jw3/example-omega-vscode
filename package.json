{
  "name": "example-omega-vscode",
  "version": "0.0.1",
  "description": "Omega Edit VS Code example",
  "publisher": "jw3",
  "license": "Unlicense",
  "repository": "https://github.com/jw3/example-omega-vscode",
  "main": "out/ext.js",
  "activationEvents": [
    "onCommand:omega.grpc",
    "onCommand:omega.websocket"
  ],
  "engines": {
    "vscode": "^1.60.0"
  },
  "scripts": {
    "build": "yarn prebuild && yarn build:typescript",
    "build:typescript": "tsc -p .",
    "build:proto": "./compile-proto.sh",
    "prebuild": "yarn run build:proto",
    "package": "yarn run build && vsce package",
    "vscode:prepublish": "yarn run build"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.2.2",
    "google-protobuf": "^3.14.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^17.0.0",
    "@types/vscode": "^1.60.0",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.3.0",
    "glob": "^7.1.6",
    "mocha": "^8.3.2",
    "prettier": "^2.3.2",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.3",
    "vsce": "^1.87.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "ws": "^8.5.0",
    "@types/google-protobuf": "^3.7.4",
    "grpc-tools": "^1.10.0",
    "grpc_tools_node_protoc_ts": "^5.2.2"
  },
  "contributes": {
    "commands": [
      {
        "command": "omega.grpc",
        "title": "gRPC Omega Edit Ω",
        "category": "Omega"
      },
      {
        "command": "omega.websocket",
        "title": "WebSocket Omega Edit Ω",
        "category": "Omega"
      }
    ],
    "commandPalette": [
      {
        "command": "omega.grpc"
      },
      {
        "command": "omega.websocket"
      }
    ]
  }
}
